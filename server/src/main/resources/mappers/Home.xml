<?xml version ="1.0" encoding ="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.count.server.dao.IHomeMapper">
    <select id="labels" resultType="com.count.server.model.HomeDto">
        select * from home;
    </select>
    <select id="num" resultType="java.lang.Integer">
        select count(1) from home;


    </select>
    <insert id="add" parameterType="com.count.server.model.HomeDto">
        insert into home(label,custom,`count`,comment,`type`,cus_date)values(#{label},#{custom},#{count},#{comment},#{type},#{cus_date});
    </insert>
</mapper>

